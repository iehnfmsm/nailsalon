<h1>トップページ</h1>
<%if user_signed_in?%>
  <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
<%else%>
  <%= link_to "新規登録ページ", new_user_registration_path%>
  <%= link_to "ログインページ", new_user_session_path%>
<%end%>

<%if user_signed_in?%>
  <h2>希望日一覧</h2>
  <% @user_prefers.each do |prefer| %>
  <div>
    <%= prefer.prefer.wday + " : " + prefer.prefer.time%>
  </div>
  <%end%>
<%end%>

